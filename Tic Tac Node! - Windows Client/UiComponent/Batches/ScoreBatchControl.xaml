<UserControl x:Class="Tic_Tac_Node__Windows_Client.UiComponent.Batches.ScoreBatchControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Tic_Tac_Node__Windows_Client.UiComponent.Batches"
             mc:Ignorable="d"
             d:DesignHeight="35" x:Name="ScoreBatch">
    <Grid>
        <Label Grid.Column="0" Height="35" Width="Auto" Style="{StaticResource BlackLabel}">
            <ContentControl Content="{Binding ScoreBatchContent, ElementName=ScoreBatch}"/>
        </Label>
    </Grid>
</UserControl>